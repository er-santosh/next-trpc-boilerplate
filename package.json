{
  "name": "next-trpc-boilerplate",
  "version": "1.0.0",
  "private": true,
  "author": "Santosh Tharu (https://github.com/er-santosh)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:next": "cross-env NODE_NO_WARNINGS=1 next dev --turbopack",
    "dev": "run-p dev:*",
    "build": "cross-env NODE_NO_WARNINGS=1 next build",
    "start": "cross-env NODE_NO_WARNINGS=1 next start",
    "build-stats": "cross-env ANALYZE=true pnpm run build",
    "clean": "rimraf .next .swc out coverage",
    "lint:next": "cross-env NODE_NO_WARNINGS=1 next lint",
    "lint:tjs": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache --cache-strategy=content --cache-location=.eslintjscache --max-warnings 0",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache --max-warnings 0",
    "lint": "run-s lint:tjs lint:css",
    "lint:tjs:fix": "eslint \"**/*.{js,mjs,ts,tsx}\" --fix --cache --cache-strategy=content --cache-location=.eslintjscache --max-warnings 0",
    "lint:css:fix": "stylelint \"**/*.css\" --fix --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache --max-warnings 0",
    "lint:fix": "run-s lint:tjs:fix lint:css:fix",
    "validate": "run-p type-check lint",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "format": "run-s lint:fix prettier:fix",
    "type-check": "tsc --noEmit --pretty",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "cz"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "lucide-react": "^0.483.0",
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.2.3",
    "@tailwindcss/postcss": "^4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.27.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-mdx": "^3.2.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-embed": "^0.5.0",
    "prettier-plugin-sql": "^0.18.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "stylelint": "^16.16.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/er-santosh/next-trpc-boilerplate"
  },
  "homepage": "https://github.com/er-santosh/next-trpc-boilerplate",
  "keywords": [
    "next",
    "nextjs",
    "next.js",
    "next-intl",
    "next-auth",
    "i18n",
    "trpc",
    "boilerplate",
    "template",
    "starter",
    "typescript",
    "tailwindcss",
    "tailwind",
    "prisma",
    "postgres",
    "sql",
    "orm",
    "drizzle",
    "trpc",
    "react-query",
    "react",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "storybook",
    "playwright",
    "jest",
    "testing-library",
    "commitizen",
    "semantic-release",
    "spotlight",
    "argon2",
    "ioredis",
    "redis",
    "luxon",
    "zod",
    "superjson",
    "tailwindcss",
    "tailwind",
    "stylelint"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
